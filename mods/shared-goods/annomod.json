{
  "src": ".",
  "out": "${annoMods}/${modName}",  
  "converter": [
    {
      "action": "static",
      "pattern": "**/*.{cfg,ifo,prp,fc,rdm,dds,include.xml}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,banner.png,README.md}"
    },
    {
      "action": "cf7",
      "pattern": "**/*.cf7"
    },
    {
      "action": "static",
      "pattern": "**/icon_*.png"
    },
    {
      "action": "gltf",
      "pattern": "**/!(propsonly)*.gltf",
      "lods": 4,
      "changePath": "rdm/",
      "animPath": "anim/",
      "plantPattern": "hemp_field.gltf"
    },
    {
      "action": "cfgyaml",
      "pattern": "**/*.cfg.yaml"
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,height,metal,mask}.png",
      "lods": 3,
      "changePath": "maps/"
    },
    {
      "action": "modinfo",
      "content_en": true
    }
  ],
  "modinfo": {
    "Version": "2.0",
    "ModID": "jakob_extra_goods",
    "IncompatibleIds": [],
    "ModDependencies": [ "jakob_ground_textures" ],
    "Category": {
      "English": "Shared"
    },
    "ModName": {
      "English": "Extra Goods (Jakob)"
    },
    "Description": {
      "English": "README.md",
      "German": "README.md"
    },
    "KnownIssues": null,
    "DLCDependencies": [
      {
        "DLC": "Docklands",
        "Dependant": "partly"
      }
    ],
    "CreatorName": "Jakob",
    "CreatorContact": "https://github.com/jakobharder/anno-1800-jakobs-mods",
    "Image" : null
  }
}