{
  "src": ".",
  "out": "${annoMods}/${modName}",  
  "converter": [
    {
      "action": "static",
      "pattern": "**/*.{cfg,ifo,prp,fc,rdm,dds}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,**/icons/*.png,banner.png,README.md}"
    },
    {
      "action": "cf7",
      "pattern": "**/*.cf7"
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,height,metal}.png"
    },
    {
      "action": "gltf",
      "pattern": "**/*.{glb,gltf}"
    },
    {
      "action": "cfgyaml",
      "pattern": "**/*.cfg.yaml"
    },
    {
      "action": "modinfo",
      "content_en": true
    }
  ],
  "modinfo": {
    "Version": "1.4.1",
    "ModID": "jakob_Biogas_Plant",
    "IncompatibleIds": [],
    "ModDependencies": null,
    "Category": {
      "English": "Building"
    },
    "ModName": {
      "English": "Biogas Production (Jakob)"
    },
    "Description": {
      "English": "README.md"
    },
    "KnownIssues": null,
    "DLCDependencies": [
      {
        "DLC": "ThePassage",
        "Dependant": "required"
      }
    ],
    "CreatorName": "Jakob",
    "CreatorContact": null,
    "Image" : null
  }
}