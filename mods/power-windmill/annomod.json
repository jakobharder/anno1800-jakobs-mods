{
  "src": ".",
  "out": "${annoMods}/${modName}",  
  "converter": [
    {
      "action": "static",
      "pattern": "**/*.{cfg,ifo,prp,fc,rdm,dds,include.xml}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,**/icon_*.png,banner.png,README.md}"
    },
    {
      "action": "cf7",
      "pattern": "**/*.cf7"
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,height,metal}.png",
      "lods": 3
    },
    {
      "action": "gltf",
      "pattern": "**/*.gltf"
    },
    {
      "action": "cfgyaml",
      "pattern": "**/*.cfg.yaml"
    },
    {
      "action": "modinfo",
      "content_en": true
    }
  ],
  "modinfo": {
    "Version": "5.1",
    "ModID": "jakob_Nates_Windmill",
    "IncompatibleIds": [],
    "DeprecateIds": [ "jakob_bugfix_mill_wind_direction" ],
    "ModDependencies": null,
    "Category": {
      "English": "Building"
    },
    "ModName": {
      "English": "Nate's Windmill (Jakob)"
    },
    "Description": {
      "English": "README.md",
      "German": "README.md"
    },
    "KnownIssues": null,
    "DLCDependencies": [ ],
    "CreatorName": "Jakob",
    "CreatorContact": "https://github.com/jakobharder/anno-1800-jakobs-mods",
    "Image" : null
  }
  
}