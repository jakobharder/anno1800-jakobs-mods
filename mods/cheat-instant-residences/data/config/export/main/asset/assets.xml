    </ModOp>
    <ModOp Type="merge" GUID="112091" Path="/Values/Constructable">
      <Constructable>
        <ConstructionMenuIcon>1500011573</ConstructionMenuIcon>
      </Constructable>
    </ModOp>
  </Group>

